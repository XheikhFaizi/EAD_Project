@using NET_QR.Models;

@model List<UserQrRelation>;

 @{
    ViewBag.Title = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";

}



<!-- ======= Portfolio Section ======= -->
    <section id="portfolio" class="portfolio">
      <div class="container" data-aos="fade-up">

        <div class="section-title">
          <h2>Portfolio</h2>
          <p>Check Out Your Qrs</p>
        </div>

        <div class="row" data-aos="fade-up" data-aos-delay="100">
          <div class="col-lg-12">
           
          </div>
        </div>

        <div class="row portfolio-container" data-aos="fade-up" data-aos-delay="200">
            @foreach(var item in Model)
            {
             
                <div class="col-lg-4 col-md-6 portfolio-item filter-app">
            <div class="portfolio-wrap">
              <img src=@Url.Content(item.imagepath) class="img-fluid" alt="">
              <div class="portfolio-links">
                <a href="@Url.Content(item.imagepath)" data-gallery="portfolioGallery" class="portfolio-lightbox" title="Download" download><i class="fa fa-link" aria-hidden="true"></i></a>
             
              </div>
              <div class="portfolio-info">
                <h4>@item.name</h4>
                <p>App</p>
              </div>
            </div>
          </div>
            }

        </div>

      </div>
    </section><!-- End Portfolio Section -->